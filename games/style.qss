QWidget {
	background: #4C4C4C;
}

QPushButton, QSpinBox {
    border: 1px solid black;
    background-color: #606060;
    padding: 5px;
    border-radius: 5px;
    color: #CCCCCC;
}

QLabel {
    color: #CCCCCC;
}

QSlider, QMediaPlayer {
    background: #4C4C4C;
}

QSpinBox::up-button, QSpinBox::down-button {
    width: 0;
}

VideoControls > QPushButton {
    height: 30px;
    padding: 0;
    margin: 0;
    padding-top: 5px;
    border: 1px solid black;
    background-color: #606060;
    border-radius: 5px;
    color: #CCCCCC;
}